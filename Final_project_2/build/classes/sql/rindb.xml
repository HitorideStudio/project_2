<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="placelistdb">
	<insert id="rindbinsert" parameterType="vo">
	   insert into placelist values(#{placename}, #{address}, #{tel}, #{lon}, #{lat},#{place})
	</insert>
	<select id="selectplacelist" resultType="vo">
		select * from placelist
	</select>
</mapper>